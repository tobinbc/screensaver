{
  "compilerOptions": {
    "esModuleInterop": true,
    "target": "es2018",
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "watch": false,
   
    "types": [
      "chrome"
    ],
    "lib": [
      "es2018",
      "dom",
      "dom.iterable"
    ]
  },
  "exclude": [
    "node_modules/**/*",
    "app/node_modules/**/*"
  ]
}